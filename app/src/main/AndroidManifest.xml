<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.justlisten">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Movie1"
            android:label="Movie1" />
        <activity
            android:name=".Movie1Song1"
            android:label="Movie1 Song1" />
        <activity
            android:name=".Movie1Song2"
            android:label="Movie1 Song2" />
        <activity
            android:name=".Search"
            android:label="Search" />
        <activity
            android:name=".NewReleased"
            android:label="New Released"/>
        <activity
            android:name=".PlayList"
            android:label="Play List">
        </activity>
    </application>

</manifest>